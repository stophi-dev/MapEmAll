{
    "name": "MapEmAll",
    "version": "0.1.0",
    "keywords": [
        "library",
        "map",
        "geo",
        "googlemaps",
        "bingmaps",
        "openstreetmaps"
    ],
    "author": "Philip St√∂hrer",
    "contributors": [],
    "dependencies": {},
    "devDependencies": {
        "chai": "^3.4.0",
        "codecov.io": "^0.1.6",
        "jshint": "^2.9.1",
        "karma": "^0.13.15",
        "karma-chai": "^0.1.0",
        "karma-chai-sinon": "^0.1.5",
        "karma-coverage": "*",
        "karma-htmlfile-reporter": "^0.2.2",
        "karma-mocha": "^0.2.0",
        "karma-mocha-reporter": "^1.1.5",
        "karma-phantomjs-launcher": "^0.2.1",
        "karma-requirejs": "^0.2.2",
        "karma-sinon": "^1.0.4",
        "mocha": "^2.3.3",
        "phantomjs": "*",
        "requirejs": "^2.1.22",
        "sinon": "^1.17.2",
        "sinon-chai": "^2.8.0"
    },
    "scripts": {
        "build": "r.js -o build/build.json optimize=none out=dist/MapEmAll.js && r.js -o  build/build.json out=dist/MapEmAll.min.js",
        "test": "karma start",
        "jshint": "jshint src",
        "codecov.io": "cat ./results/coverage/lcov/lcov.info | ./node_modules/codecov.io/bin/codecov.io.js",
        "update-helloworld": "cp dist/MapEmAll.js examples/HelloWorld/public_html"
    },
    "jshintConfig": {
        "bitwise": true,
        "browser": true,
        "curly": true,
        "eqeqeq": true,
        "nocomma": true,
        "freeze": true,
        "funcscope": true,
        "futurehostile": true,
        "globals": {
            "define": true,
            "require": true
        },
        "maxcomplexity": 5,
        "maxdepth": 2,
        "maxparams": 3,
        "noarg": true,
        "notypeof": true,
        "singleGroups": true,
        "strict": true,
        "undef": true,
        "unused": true
    }
}
